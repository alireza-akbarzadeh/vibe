{
  "name": "vibe",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:vite\" \"npm:dev:ngrok\" \"npm:db:studio\"",
    "dev:vite": "vite dev --port 3000 | pino-pretty",
    "postinstall": "prisma generate",
    "dev:ngrok": "ngrok start vibe --config ngrok.yml",
    "dev:raw": "vite dev --port 3000",
    "build": "prisma generate && vite build --ssr",    "vercel-build": "prisma generate && prisma migrate deploy && vite build",    "preview": "vite preview",
    "test": "vitest run",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check",
    "start": "node --import ./.output/server/instrument.server.mjs .output/server/index.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check:fix": "biome check --write --unsafe ./src",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset"
  },
